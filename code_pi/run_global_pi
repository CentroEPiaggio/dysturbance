#!/usr/bin/octave -qf

% Dysturbance - EuroBench Consortium
% Created By: Simone Monteleone
% mail: simone.monteleone@phd.unipi.it

fprintf("current directory: %s\n", pwd())
addpath("src")
arg_list = argv();
if nargin ~= 2
  fprintf("[ERROR] There should be 2 args:\n")
  fprintf("         * a name for the directory where the results will be stored;\n")
  fprintf("         * a protocol number.\n\n");
  fprintf("Usage:   ./run_global_pi output_dir 1\n\n");
  exit(127);
endif

fprintf("data folder: %s\n", arg_list{1});
fprintf("protocol: %s\n", arg_list{2})

Compute_Global_PI(arg_list{1}, arg_list{2});
